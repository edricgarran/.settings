[color]
    ui = true
[user]
    email = felipe.trzaskowski@gmail.com
    name = Felipe Trzaskowski
[pull]
    rebase = true
[alias]
    tree = log --color --all --branches --graph --decorate --oneline
    sync = !git pull --rebase && git push
[merge]
	conflictstyle = diff3
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
